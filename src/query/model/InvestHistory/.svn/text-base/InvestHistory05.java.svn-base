package query.model.InvestHistory;

import java.util.HashMap;

public class InvestHistory05 {

	private String r_invs_ad_sub_desc;
	private String r_invs_code;
	private String r_invs_tr_date;	
	private String r_get_date;

	/**
	 * @param hsmp
	 */
	public InvestHistory05(HashMap<String, String> hsmp) {
		// TODO Auto-generated constructor stub
		r_invs_ad_sub_desc = (hsmp.get("r_invs_ad_sub_desc") == null) ? ""
				: hsmp.get("r_invs_ad_sub_desc").trim();
		r_invs_code = (hsmp.get("r_invs_code") == null) ? "" : hsmp.get(
				"r_invs_code").trim();
		r_invs_tr_date = (hsmp.get("r_invs_tr_date") == null) ? "" : hsmp.get(
				"r_invs_tr_date").trim();		
		r_get_date = (hsmp.get("r_get_date") == null) ? "" : hsmp.get(
				"r_get_date").trim();
	}

	public String getR_invs_ad_sub_desc() {
		return r_invs_ad_sub_desc;
	}

	public void setR_invs_ad_sub_desc(String r_invs_ad_sub_desc) {
		this.r_invs_ad_sub_desc = r_invs_ad_sub_desc;
	}

	public String getR_invs_code() {
		return r_invs_code;
	}

	public void setR_invs_code(String r_invs_code) {
		this.r_invs_code = r_invs_code;
	}

	public String getR_invs_tr_date() {
		return r_invs_tr_date;
	}

	public void setR_invs_tr_date(String r_invs_tr_date) {
		this.r_invs_tr_date = r_invs_tr_date;
	}
	
	public String getR_get_date() {
		return r_get_date;
	}

	public void setR_get_date(String r_get_date) {
		this.r_get_date = r_get_date;
	}

}
