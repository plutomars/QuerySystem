package query.model.NewFaceAmount;

import java.util.HashMap;
/**
 * 
 * @author 陳冠瑜
 * 
 */
public class NewFaceAmount01 {
	private String r_result_ind;  // 查詢結果
	private String r_plan_desc;   // 險種類別
	private String r_face_amt;    // 累計保額
	private String r_unit_1;      // 單位
	private String r_avail_amt;   // 目前尚可投保額度
	private String r_unit_2;      // 單位
	private String r_desc1;      //
	private String r_desc2;      //
	private String r_desc3;      //
	private String r_desc4;      //

	public NewFaceAmount01(HashMap<String, String> hsmp) {
		r_result_ind = (hsmp.get("r_result_ind") == null) ? "" : hsmp.get(
				"r_result_ind").trim();
		r_plan_desc = (hsmp.get("r_plan_desc") == null) ? "" : hsmp.get(
				"r_plan_desc").trim();
		r_face_amt = (hsmp.get("r_face_amt") == null) ? "" : hsmp.get(
				"r_face_amt").trim();
		r_unit_1 = (hsmp.get("r_unit_1") == null) ? "" : hsmp.get("r_unit_1")
				.trim();
		r_avail_amt = (hsmp.get("r_avail_amt") == null) ? "" : hsmp.get(
				"r_avail_amt").trim();
		r_unit_2 = (hsmp.get("r_unit_2") == null) ? "" : hsmp.get("r_unit_2")
				.trim();
		r_desc1 = (hsmp.get("r_desc1") == null) ? "" : hsmp.get("r_desc1")
				.trim();
		r_desc2 = (hsmp.get("r_desc2") == null) ? "" : hsmp.get("r_desc2")
				.trim();
		r_desc3 = (hsmp.get("r_desc3") == null) ? "" : hsmp.get("r_desc3")
				.trim();
		r_desc4 = (hsmp.get("r_desc4") == null) ? "" : hsmp.get("r_desc4")
				.trim();
	}

	public String getR_result_ind() {
		return r_result_ind;
	}

	public void setR_result_ind(String r_result_ind) {
		this.r_result_ind = r_result_ind;
	}

	public String getR_plan_desc() {
		return r_plan_desc;
	}

	public void setR_plan_desc(String r_plan_desc) {
		this.r_plan_desc = r_plan_desc;
	}

	public String getR_face_amt() {
		return r_face_amt;
	}

	public void setR_face_amt(String r_face_amt) {
		this.r_face_amt = r_face_amt;
	}

	public String getR_unit_1() {
		return r_unit_1;
	}

	public void setR_unit_1(String r_unit_1) {
		this.r_unit_1 = r_unit_1;
	}

	public String getR_avail_amt() {
		return r_avail_amt;
	}

	public void setR_avail_amt(String r_avail_amt) {
		this.r_avail_amt = r_avail_amt;
	}

	public String getR_unit_2() {
		return r_unit_2;
	}

	public void setR_unit_2(String r_unit_2) {
		this.r_unit_2 = r_unit_2;
	}
	
	public String getR_desc1() {
		return r_desc1;
	}

	public void setR_desc1(String r_desc1) {
		this.r_desc1 = r_desc1;
	}
	
	public String getR_desc2() {
		return r_desc2;
	}

	public void setR_desc2(String r_desc2) {
		this.r_desc2 = r_desc2;
	}
	
	public String getR_desc3() {
		return r_desc3;
	}

	public void setR_desc3(String r_desc3) {
		this.r_desc3 = r_desc3;
	}
	
	public String getR_desc4() {
		return r_desc4;
	}

	public void setR_desc4(String r_desc4) {
		this.r_desc4 = r_desc4;
	}
	
}
