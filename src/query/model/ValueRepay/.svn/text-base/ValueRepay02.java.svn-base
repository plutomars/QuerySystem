package query.model.ValueRepay;

import java.util.HashMap;

public class ValueRepay02 {
	private String r_result_ind;
	private String r_cp_anniv_date;
	private String r_plan_code;
	private String r_rate_scale;
	private String r_nonresp_sw; // 還本註記
	private String r_cp_sts_desc;
	private String r_chk_sw;
	private String r_cp_amt;
	private String r_add_amt;
	private String r_sub_amt;
	private String r_cp_pay_amt;
	private String r_pay_type;
	private String r_cp_return_date_interval; // 還本回流回覆期限

	public ValueRepay02(HashMap<String, String> hsmp) {
		r_result_ind = (hsmp.get("r_result_ind") == null) ? "" : hsmp.get(
				"r_result_ind").trim();
		r_cp_anniv_date = (hsmp.get("r_cp_anniv_date") == null) ? "" : hsmp
				.get("r_cp_anniv_date").trim();
		r_plan_code = (hsmp.get("r_plan_code") == null) ? "" : hsmp.get(
				"r_plan_code").trim();
		r_rate_scale = (hsmp.get("r_rate_scale") == null) ? "" : hsmp.get(
				"r_rate_scale").trim();
		r_nonresp_sw = (hsmp.get("r_nonresp_sw") == null) ? "" : hsmp.get(
				"r_nonresp_sw").trim(); // 還本註記
		r_cp_sts_desc = (hsmp.get("r_cp_sts_desc") == null) ? "" : hsmp.get(
				"r_cp_sts_desc").trim();
		r_chk_sw = (hsmp.get("r_chk_sw") == null) ? "" : hsmp.get("r_chk_sw")
				.trim();
		r_cp_amt = (hsmp.get("r_cp_amt") == null) ? "" : hsmp.get("r_cp_amt")
				.trim();
		r_add_amt = (hsmp.get("r_add_amt") == null) ? "" : hsmp
				.get("r_add_amt").trim();
		r_sub_amt = (hsmp.get("r_sub_amt") == null) ? "" : hsmp
				.get("r_sub_amt").trim();
		r_cp_pay_amt = (hsmp.get("r_cp_pay_amt") == null) ? "" : hsmp.get(
				"r_cp_pay_amt").trim();
		r_pay_type = (hsmp.get("r_pay_type") == null) ? "" : hsmp.get(
				"r_pay_type").trim();
		r_cp_return_date_interval = (hsmp.get("r_cp_return_date_interval") == null) ? ""
				: hsmp.get("r_cp_return_date_interval").trim();
	}

	public String getResult_ind() {
		return r_result_ind;
	}

	public void setResult_ind(String result_ind) {
		this.r_result_ind = result_ind;
	}

	public String getR_cp_anniv_date() {
		return r_cp_anniv_date;
	}

	public void setR_cp_anniv_date(String r_cp_anniv_date) {
		this.r_cp_anniv_date = r_cp_anniv_date;
	}

	public String getR_plan_code() {
		return r_plan_code;
	}

	public void setR_plan_code(String r_plan_code) {
		this.r_plan_code = r_plan_code;
	}

	public String getR_rate_scale() {
		return r_rate_scale;
	}

	public void setR_rate_scale(String r_rate_scale) {
		this.r_rate_scale = r_rate_scale;
	}

	public String getR_nonresp_sw() {
		return r_nonresp_sw;
	}

	public void setR_nonresp_sw(String r_nonresp_sw) {
		this.r_nonresp_sw = r_nonresp_sw;
	}

	public String getR_cp_sts_desc() {
		return r_cp_sts_desc;
	}

	public void setR_cp_sts_desc(String r_cp_sts_desc) {
		this.r_cp_sts_desc = r_cp_sts_desc;
	}

	public String getR_chk_sw() {
