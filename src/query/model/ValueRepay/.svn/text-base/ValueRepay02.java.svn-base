	}

	public void setR_add_amt(String r_add_amt) {
		this.r_add_amt = r_add_amt;
	}

	public String getR_sub_amt() {
		return r_sub_amt;
	}

	public void setR_sub_amt(String r_sub_amt) {
		this.r_sub_amt = r_sub_amt;
	}

	public String getR_cp_pay_amt() {
		return r_cp_pay_amt;
	}

	public void setR_cp_pay_amt(String r_cp_pay_amt) {
		this.r_cp_pay_amt = r_cp_pay_amt;
	}

	public String getR_pay_type() {
		return r_pay_type;
	}

	public void setR_pay_type(String r_pay_type) {
		this.r_pay_type = r_pay_type;
