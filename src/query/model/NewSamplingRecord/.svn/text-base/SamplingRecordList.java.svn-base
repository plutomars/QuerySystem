package query.model.NewSamplingRecord;

import java.util.HashMap;

/**
 * @ * @author YIHUA
 * 
 * @version 1.0<br>
 *          100/10/06<br>
 *          抽檢記錄查詢清單(JavaBean)
 * @since 1.0
 */

public class SamplingRecordList {

	private String r_policy_no; // 保單號碼
	private String r_po_sts_code; // 保單狀態
	private String r_po_sts_desc; // 保單狀態
	private String r_app_input_date; // 抽檢日期
	private String r_medical_desc; // 體檢狀態
	private String r_immune; // 豁免
	
	
	public SamplingRecordList(HashMap<String, String> hsmp) {

		r_policy_no = (hsmp.get("r_policy_no") == null) ? "" : hsmp.get(
				"r_policy_no").trim();
		r_po_sts_code = (hsmp.get("r_po_sts_code") == null) ? "" : hsmp.get(
				"r_po_sts_code").trim();
		r_po_sts_desc = (hsmp.get("r_po_sts_desc") == null) ? "" : hsmp.get(
				"r_po_sts_desc").trim();
		r_app_input_date = (hsmp.get("r_app_input_date") == null) ? "" : hsmp
				.get("r_app_input_date").trim();
		r_medical_desc = (hsmp.get("r_medical_desc") == null) ? "" : hsmp.get(
				"r_medical_desc").trim();
		r_immune = (hsmp.get("r_immune") == null) ? "" : hsmp.get(
				"r_immune").trim();
				
	}
	
	public SamplingRecordList(){
	}
	

	public String getR_policy_no() {
		return r_policy_no;
	}

	public void setR_policy_no(String r_policy_no) {
		this.r_policy_no = r_policy_no;
	}

	public String getR_po_sts_code() {
		return r_po_sts_code;
	}

	public void setR_po_sts_code(String r_po_sts_code) {
		this.r_po_sts_code = r_po_sts_code;
	}

	public String getR_po_sts_desc() {
		return r_po_sts_desc;
	}

	public void setR_po_sts_desc(String r_po_sts_desc) {
		this.r_po_sts_desc = r_po_sts_desc;
	}
	
	
	public String getR_app_input_date() {
		return r_app_input_date;
	}

	public void setR_app_input_date(String r_app_input_date) {
		this.r_app_input_date = r_app_input_date;
	}

	public String getR_medical_desc() {
		return r_medical_desc;
	}

	public void setR_medical_desc(String r_medical_desc) {
		this.r_medical_desc = r_medical_desc;
	}
	
	public String getR_immune() {
		return r_immune;
	}

	public void setR_immune(String r_immune) {
		this.r_immune = r_immune;
	}
	
	
}
