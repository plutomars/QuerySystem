package query.model;

public class QueryPoClient extends AbstractModelObject{
	private String m_name;
	private String m_policy_no;
	public QueryPoClient(){
		
	}
	public QueryPoClient(String name,String policy_no){
		m_name = name;
		m_policy_no = policy_no;
	}
	public String getName() {
		return m_name;
	}
	public void setName(String name) {
		String oldValue = m_name;
		m_name = name;
		firePropertyChange("name", oldValue, m_name);
	}
	public String getPolicy_no() {
		return m_policy_no;
	}
	public void setPolicy_no(String policy_no) {
		String oldValue = m_policy_no;
		m_policy_no = policy_no;
		firePropertyChange("policy_no", oldValue, m_policy_no);
		
	}
}
