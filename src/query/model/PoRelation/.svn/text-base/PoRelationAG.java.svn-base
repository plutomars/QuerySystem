package query.model.PoRelation;

import java.util.HashMap;

/**
 * @ * @author FYYANG
 * 
 * @version 1.0<br>
 *          100/07/28<br>
 *          業務員(JavaBean)
 * @since 1.0
 */
public class PoRelationAG {
	private String r_result_ind;
	private String r_relation_desc; // 關係
	private String r_agent_code; // 業務員ID
	private String r_dept_code; // 營業處
	private String r_level; // 職級
	private String r_mobile; // 行動電話
	private String r_tel1; // 電話1
	private String r_tel2; // 營業處電話
	private String r_address; // 地址
	private String r_agent_name; // 業務員名字
	private String r_comm_share; // 佣金比例
	private String r_writing_class; // 招攬授權分類
	private String r_agent_no; // 業務證號

	public PoRelationAG(HashMap<String, String> hsmp) {
		r_result_ind = (hsmp.get("r_result_ind") == null) ? "" : hsmp.get(
				"r_result_ind").trim();
		r_relation_desc = (hsmp.get("r_relation_desc") == null) ? "" : hsmp
				.get("r_relation_desc").trim();
		r_agent_code = (hsmp.get("r_agent_code") == null) ? "" : hsmp.get(
				"r_agent_code").trim();
		r_dept_code = (hsmp.get("r_dept_code") == null) ? "" : hsmp.get(
				"r_dept_code").trim();
		r_level = (hsmp.get("r_level") == null) ? "" : hsmp.get("r_level")
				.trim();
		r_mobile = (hsmp.get("r_mobile") == null) ? "" : hsmp.get("r_mobile")
				.trim();
		r_tel1 = (hsmp.get("r_tel1") == null) ? "" : hsmp.get("r_tel1").trim();
		r_tel2 = (hsmp.get("r_tel2") == null) ? "" : hsmp.get("r_tel2").trim();
		r_address = (hsmp.get("r_address") == null) ? "" : hsmp
				.get("r_address").trim();
		r_agent_name = (hsmp.get("r_agent_name") == null) ? "" : hsmp.get(
				"r_agent_name").trim();
		r_comm_share = (hsmp.get("r_comm_share") == null) ? "" : hsmp.get(
				"r_comm_share").trim();
		r_writing_class = (hsmp.get("r_writing_class") == null) ? "" : hsmp
				.get("r_writing_class").trim();
		r_agent_no = (hsmp.get("r_agent_no") == null) ? "" : hsmp
				.get("r_agent_no").trim();
	}

	public String getresult_ind() {
		return r_result_ind;
	}

	public void setresult_ind(String r_result_ind) {
		this.r_result_ind = r_result_ind;
	}

	public String getRelation_desc() {
		return r_relation_desc;
	}

	public void setRelation_desc(String relation_desc) {
		this.r_relation_desc = relation_desc;
	}

	public String getAgent_code() {
		return r_agent_code;
	}

	public void setAgent_code(String agent_code) {
		this.r_agent_code = agent_code;
	}

	public String getDept_code() {
		return r_dept_code;
	}

	public void setDept_code(String dept_code) {
		this.r_dept_code = dept_code;
	}

	public String getLevel() {
		return r_level;
	}

	public void setLevel(String level) {
		this.r_level = level;
	}

	public String getMobile() {
		return r_mobile;
	}

	public void setMobile(String mobile) {
		this.r_mobile = mobile;
	}

	public String getTel1() {
		return r_tel1;
	}

	public void setTel1(String tel1) {
		this.r_tel1 = tel1;
	}

	public String getTel2() {
		return r_tel2;
	}

	public void setTel2(String tel2) {
		this.r_tel2 = tel2;
	}

	public String getAddress() {
		return r_address;
	}

	public void setAddress(String address) {
		this.r_address = address;
	}

	public String getComm_share() {
		return r_comm_share;
	}

	public void setComm_share(String comm_share) {
		this.r_comm_share = comm_share;
	}

	public String getAgent_name() {
		return r_agent_name;
	}

	public void setAgent_name(String agent_name) {
		this.r_agent_name = agent_name;
	}

	public String getWriting_class() {
		return r_writing_class;
	}

	public void setWriting_class(String writing_class) {
		this.r_writing_class = writing_class;
	}

	public String getR_agent_no() {
		return r_agent_no;
	}

	public void setR_agent_no(String r_agent_no) {
		this.r_agent_no = r_agent_no;
	}
}
