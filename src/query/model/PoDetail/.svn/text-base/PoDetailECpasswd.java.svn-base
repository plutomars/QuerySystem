import java.util.HashMap;

public class PoDetailECpasswd {
    private String result_ind;
	private String pwd_sw;
	private String pwd_sw_desc;
	private String accu_lcnt;
	private String accu_ecnt  ;

	public PoDetailECpasswd(HashMap<String, String> hsmp) {
		result_ind = (hsmp.get("r_result_ind") == null) ? "" : hsmp.get(
				"r_result_ind").trim();
		pwd_sw = (hsmp.get("r_pwd_sw") == null) ? "" : hsmp.get("r_pwd_sw").trim();
		pwd_sw_desc = (hsmp.get("r_pwd_sw_desc") == null) ? "" : hsmp.get("r_pwd_sw_desc").trim();
		accu_lcnt = (hsmp.get("r_accu_lcnt") == null) ? "" : hsmp.get("r_accu_lcnt").trim();
		accu_ecnt   = (hsmp.get("r_accu_ecnt") == null) ? "" : hsmp.get("r_accu_ecnt").trim();
	}
	public String getResult_ind() {
		return result_ind;
	}

	public void setResult_ind(String result_ind) {
		this.result_ind = result_ind;
	}
    public String getpwd_sw() {
        return pwd_sw; }
    public void setpwd_sw(String pwd_sw) {
        this.pwd_sw = pwd_sw; }

	public String getpwd_sw_desc() {
        return pwd_sw_desc; }
	public void setpwd_sw_desc(String pwd_sw_desc) {
        this.pwd_sw_desc = pwd_sw_desc; }

	public String getaccu_lcnt() {
        return accu_lcnt; }
	public void setaccu_lcnt(String accu_lcnt) {
        this.accu_lcnt = accu_lcnt; }

	public String getaccu_ecnt() {
        return accu_ecnt; }
	public void setaccu_ecnt(String accu_ecnt) {
        this.accu_ecnt = accu_ecnt; }


}
