package query.model.PayQuery;

import java.util.HashMap;

import query.model.AbstractModelObject;

public class PayQuery02 extends AbstractModelObject {
	private String m_voucher_no;   //傳票
	private String m_voucher_seq;  //序號
	private String m_acct_no;      //會計科目
	private String m_dept_code;    //部門別
	private String m_journal_amt;  //分錄金額
	private String m_recn_code;    //沖銷碼
	private String m_policy_no;    //保單號碼	
	private String m_agent_code;   //業務代碼
	private String m_reason_code;  //原因碼
	private String m_journal_desc; //分錄摘要
	
	public PayQuery02(){
	}
	public PayQuery02(HashMap<String, String> hsmp) {		
		m_voucher_no = (hsmp.get("r_voucher_no") == null) ? "" : hsmp.get("r_voucher_no").trim();
		m_voucher_seq = (hsmp.get("r_voucher_seq") == null) ? "" : hsmp.get("r_voucher_seq").trim();
		m_acct_no = (hsmp.get("r_acct_no") == null) ? "" : hsmp.get("r_acct_no").trim();
		m_dept_code = (hsmp.get("r_dept_code") == null) ? "" : hsmp.get("r_dept_code").trim();
		m_journal_amt = (hsmp.get("r_journal_amt") == null) ? "" : hsmp.get("r_journal_amt").trim();
		m_recn_code = (hsmp.get("r_recn_code") == null) ? "" : hsmp.get("r_recn_code").trim();
		m_policy_no = (hsmp.get("r_policy_no") == null) ? "" : hsmp.get("r_policy_no").trim();
		m_agent_code = (hsmp.get("r_agent_code") == null) ? "" : hsmp.get("r_agent_code").trim();
		m_reason_code = (hsmp.get("r_reason_code") == null) ? "" : hsmp.get("r_reason_code").trim();
		m_journal_desc = (hsmp.get("r_journal_desc") == null) ? "" : hsmp.get("r_journal_desc").trim();	
	}
	public String getM_voucher_no() {
		return m_voucher_no;
	}
	public void setM_voucher_no(String m_voucher_no) {
		this.m_voucher_no = m_voucher_no;
	}
	public String getM_voucher_seq() {
		return m_voucher_seq;
	}
	public void setM_voucher_seq(String m_voucher_seq) {
		this.m_voucher_seq = m_voucher_seq;
	}
	public String getM_acct_no() {
		return m_acct_no;
	}
	public void setM_acct_no(String m_acct_no) {
		this.m_acct_no = m_acct_no;
	}
	public String getM_dept_code() {
		return m_dept_code;
	}
	public void setM_dept_code(String m_dept_code) {
		this.m_dept_code = m_dept_code;
	}
	public String getM_journal_amt() {
		return m_journal_amt;
	}
	public void setM_journal_amt(String m_journal_amt) {
		this.m_journal_amt = m_journal_amt;
	}
	public String getM_recn_code() {
		return m_recn_code;
	}
	public void setM_recn_code(String m_recn_code) {
		this.m_recn_code = m_recn_code;
	}
	public String getM_policy_no() {
		return m_policy_no;
	}
	public void setM_policy_no(String m_policy_no) {
		this.m_policy_no = m_policy_no;
	}
	public String getM_agent_code() {
		return m_agent_code;
	}
	public void setM_agent_code(String m_agent_code) {
		this.m_agent_code = m_agent_code;
	}
	public String getM_reason_code() {
		return m_reason_code;
	}
	public void setM_reason_code(String m_reason_code) {
		this.m_reason_code = m_reason_code;
	}
	public String getM_journal_desc() {
		return m_journal_desc;
	}
	public void setM_journal_desc(String m_journal_desc) {
		this.m_journal_desc = m_journal_desc;
	}
	
	
	
}
