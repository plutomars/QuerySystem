package query.model.InvestCash;

import java.util.HashMap;

public class InvestCash04 {

	private String r_result_ind; // 執行結果(0:有資料,1:無資料)
	private String r_policy_year; // 保單年度
	private String r_prem_year; // 保費年度
	private String r_prem_month; // 保費月份
	private String r_target_prem; // 目標(基本)保費
	private String r_excess_prem; // 額外(超額)保費
	private String r_feld_expn_tgpm; // 費用-目標(基本)
	private String r_feld_expn_espm; // 費用-額外(超額)

	public InvestCash04() {
		// TODO Auto-generated constructor stub
	}

	public InvestCash04(HashMap<String, String> hsmp) {
		r_result_ind = (hsmp.get("r_result_ind") == null) ? "" : hsmp.get(
				"r_result_ind").trim();
		r_policy_year = (hsmp.get("r_policy_year") == null) ? "" : hsmp.get(
				"r_policy_year").trim();
		r_prem_year = (hsmp.get("r_prem_year") == null) ? "" : hsmp.get(
				"r_prem_year").trim();
		r_prem_month = (hsmp.get("r_prem_month") == null) ? "" : hsmp.get(
				"r_prem_month").trim();
		r_target_prem = (hsmp.get("r_target_prem") == null) ? "" : hsmp.get(
				"r_target_prem").trim();
		r_excess_prem = (hsmp.get("r_excess_prem") == null) ? "" : hsmp.get(
				"r_excess_prem").trim();
		r_feld_expn_tgpm = (hsmp.get("r_feld_expn_tgpm") == null) ? "" : hsmp
				.get("r_feld_expn_tgpm").trim();
		r_feld_expn_espm = (hsmp.get("r_feld_expn_espm") == null) ? "" : hsmp
				.get("r_feld_expn_espm").trim();

	}

	public String getR_result_ind() {
		return r_result_ind;
	}

	public void setR_result_ind(String r_result_ind) {
		this.r_result_ind = r_result_ind;
	}

	public String getR_policy_year() {
		return r_policy_year;
	}

	public void setR_policy_year(String r_policy_year) {
		this.r_policy_year = r_policy_year;
	}

	public String getR_prem_year() {
		return r_prem_year;
	}

	public void setR_prem_year(String r_prem_year) {
		this.r_prem_year = r_prem_year;
	}

	public String getR_prem_month() {
		return r_prem_month;
	}

	public void setR_prem_month(String r_prem_month) {
		this.r_prem_month = r_prem_month;
	}

	public String getR_target_prem() {
		return r_target_prem;
	}

	public void setR_target_prem(String r_target_prem) {
		this.r_target_prem = r_target_prem;
	}

	public String getR_excess_prem() {
		return r_excess_prem;
	}

	public void setR_excess_prem(String r_excess_prem) {
		this.r_excess_prem = r_excess_prem;
	}

	public String getR_feld_expn_tgpm() {
		return r_feld_expn_tgpm;
	}

	public void setR_feld_expn_tgpm(String r_feld_expn_tgpm) {
		this.r_feld_expn_tgpm = r_feld_expn_tgpm;
	}

	public String getR_feld_expn_espm() {
		return r_feld_expn_espm;
	}

	public void setR_feld_expn_espm(String r_feld_expn_espm) {
		this.r_feld_expn_espm = r_feld_expn_espm;
	}

}
