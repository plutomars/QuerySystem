package query.model.PosChange;

import java.util.HashMap;

public class PosChange03 {
	private String r_result_ind;
	private String r_po_chg_doc_code;
	private String r_po_chg_doc_desc;
	private String r_form_id;
	
	public PosChange03(HashMap<String,String> hsmp){
		r_result_ind = (hsmp.get("r_result_ind") == null) ? "" : hsmp.get(
				"r_result_ind").trim();
		r_po_chg_doc_code = (hsmp.get("r_po_chg_doc_code") == null) ? "" : hsmp.get(
				"r_po_chg_doc_code").trim();
		r_po_chg_doc_desc = (hsmp.get("r_po_chg_doc_desc") == null) ? "" : hsmp.get(
				"r_po_chg_doc_desc").trim();
		r_form_id = (hsmp.get("r_form_id") == null) ? "" : hsmp.get("r_form_id").trim();
	}

	public String getR_result_ind() {
		return r_result_ind;
	}

	public void setR_result_ind(String r_result_ind) {
		this.r_result_ind = r_result_ind;
	}

	public String getR_po_chg_doc_code() {
		return r_po_chg_doc_code;
	}

	public void setR_po_chg_doc_code(String r_po_chg_doc_code) {
		this.r_po_chg_doc_code = r_po_chg_doc_code;
	}

	public String getR_po_chg_doc_desc() {
		return r_po_chg_doc_desc;
	}

	public void setR_po_chg_doc_desc(String r_po_chg_doc_desc) {
		this.r_po_chg_doc_desc = r_po_chg_doc_desc;
	}		

	public String getR_form_id() {
		return r_form_id;
	}

	public void setR_form_id(String r_form_id) {
		this.r_form_id = r_form_id;
	}	
}
