package query.model.ClaimHistory;

import java.util.HashMap;
/**
 * @ * @author jnd115
 * 
 * @version 1.0<br>
 *          101/04/18<br>
 *          地檢署相驗明細(JavaBean)
 * @since 1.0
 */
public class ClaimHistory11 {
	private String r_result_ind; // 查詢結果
	private String r_zip_code; // 死亡地點郵遞區號
	private String r_address; // 死亡地點地址
	private String r_death_place; // 死亡場所類別
	private String r_death_place_desc; // 死亡場所類別說明
	private String r_death_type; // 死亡種類
	private String r_death_type_desc; // 死亡種類說明
	private String r_prosecutor_office; // 地檢署名稱序號
	private String r_office_name; // 地檢署名稱
	private String r_prosecutor_name; // 檢察官姓名
	private String r_forensic_name; // 法醫師／檢驗員姓名
	private String r_certificate_date; // 證明書開立日期

	public ClaimHistory11() {

	}

	public void setValues(HashMap<String, String> hsmp) {
		r_result_ind = (hsmp.get("r_result_ind") == null) ? "" : hsmp.get(
				"r_result_ind").trim();
		r_zip_code = (hsmp.get("r_zip_code") == null) ? "" : hsmp.get(
				"r_zip_code").trim();
		r_address = (hsmp.get("r_address") == null) ? "" : hsmp
				.get("r_address").trim();
		r_death_place = (hsmp.get("r_death_place") == null) ? "" : hsmp.get(
				"r_death_place").trim();
		r_death_place_desc = (hsmp.get("r_death_place_desc") == null) ? ""
				: hsmp.get("r_death_place_desc").trim();
		r_death_type = (hsmp.get("r_death_type") == null) ? "" : hsmp.get(
				"r_death_type").trim();
		r_death_type_desc = (hsmp.get("r_death_type_desc") == null) ? "" : hsmp
				.get("r_death_type_desc").trim();
		r_prosecutor_office = (hsmp.get("r_prosecutor_office") == null) ? ""
				: hsmp.get("r_prosecutor_office").trim();
		r_office_name = (hsmp.get("r_office_name") == null) ? "" : hsmp.get(
				"r_office_name").trim();
		r_prosecutor_name = (hsmp.get("r_prosecutor_name") == null) ? "" : hsmp
				.get("r_prosecutor_name").trim();
		r_forensic_name = (hsmp.get("r_forensic_name") == null) ? "" : hsmp
				.get("r_forensic_name").trim();
		r_certificate_date = (hsmp.get("r_certificate_date") == null) ? ""
				: hsmp.get("r_certificate_date").trim();
	}

	public String getR_result_ind() {
		return r_result_ind;
	}

	public void setR_result_ind(String r_result_ind) {
		this.r_result_ind = r_result_ind;
	}

	public String getR_zip_code() {
		return r_zip_code;
	}

	public void setR_zip_code(String r_zip_code) {
		this.r_zip_code = r_zip_code;
	}

	public String getR_address() {
		return r_address;
	}

	public void setR_address(String r_address) {
		this.r_address = r_address;
	}

	public String getR_death_place() {
		return r_death_place;
	}

	public void setR_death_place(String r_death_place) {
		this.r_death_place = r_death_place;
	}

	public String getR_death_place_desc() {
		return r_death_place_desc;
	}

	public void setR_death_place_desc(String r_death_place_desc) {
		this.r_death_place_desc = r_death_place_desc;
	}

	public String getR_death_type() {
		return r_death_type;
	}

	public void setR_death_type(String r_death_type) {
		this.r_death_type = r_death_type;
	}

	public String getR_death_type_desc() {
		return r_death_type_desc;
	}

	public void setR_death_type_desc(String r_death_type_desc) {
		this.r_death_type_desc = r_death_type_desc;
	}

	public String getR_prosecutor_office() {
		return r_prosecutor_office;
	}

	public void setR_prosecutor_office(String r_prosecutor_office) {
		this.r_prosecutor_office = r_prosecutor_office;
	}

	public String getR_office_name() {
		return r_office_name;
	}

	public void setR_office_name(String r_office_name) {
		this.r_office_name = r_office_name;
	}

	public String getR_prosecutor_name() {
		return r_prosecutor_name;
	}

	public void setR_prosecutor_name(String r_prosecutor_name) {
		this.r_prosecutor_name = r_prosecutor_name;
	}

	public String getR_forensic_name() {
		return r_forensic_name;
	}

	public void setR_forensic_name(String r_forensic_name) {
		this.r_forensic_name = r_forensic_name;
	}

	public String getR_certificate_date() {
		return r_certificate_date;
	}

	public void setR_certificate_date(String r_certificate_date) {
		this.r_certificate_date = r_certificate_date;
	}

	public String[] getCledColumn() {
		return new String[] { r_zip_code + "  " + r_address,
				r_death_place + "  " + r_death_place_desc,
				r_death_type + "  " + r_death_type_desc,
				r_prosecutor_office + "  " + r_office_name, r_prosecutor_name,
				r_forensic_name, r_certificate_date };
	}
}
