package query.model.PoBonus;

import java.util.HashMap;

import query.model.AbstractModelObject;

/**
 * @ * @author YIHUA
 * 
 * @version 1.0<br>
 *          100/08/15<br>
 *          紅利基本資料(JavaBean)
 * @since 1.0
 */

public class PoBonus extends AbstractModelObject { // **set的binding要用到**
	private String result_ind; // 1查詢結果
	private String policy_no; // 2保單號碼
	private String po_sts_code; // 3保單狀態
	private String po_sts_desc; // 4狀態說明
	private String div_option; // 5紅利選擇(英文)
	private String div_option_desc1; // 6紅利選擇(中文)
	private String applicant_name; // 7要保人name
	private String insured_name; // 8被保人name
	private String div_option_desc2; // 9紅利選擇(中文)
	private String cal_date; // 10計息日
	private String accumulated_div; // 11紅利金額
	private String div_int; // 12紅利利息
	private String div_total; // 13合計金額
	private String last_div_dur; // 14給付年度
	private String div_this_year; // 15給付紅利

	public PoBonus() {

	}

	// 以setValues的方式binding
	public void setValues(HashMap<String, String> hsmp) {

		setResult_ind((hsmp.get("r_result_ind") == null) ? "" : hsmp.get(
				"r_result_ind").trim());
		setPolicy_no((hsmp.get("r_policy_no") == null) ? "" : hsmp.get(
				"r_policy_no").trim());
		setPo_sts_code((hsmp.get("r_po_sts_code") == null) ? "" : hsmp.get(
				"r_po_sts_code").trim());
		setPo_sts_desc((hsmp.get("r_po_sts_desc") == null) ? "" : hsmp.get(
				"r_po_sts_desc").trim());
		setDiv_option((hsmp.get("r_div_option") == null) ? "" : hsmp.get(
				"r_div_option").trim());
		setDiv_option_desc1((hsmp.get("r_div_option_desc1") == null) ? ""
				: hsmp.get("r_div_option_desc1").trim());
		setApplicant_name((hsmp.get("r_applicant_name") == null) ? "" : hsmp
				.get("r_applicant_name").trim());
		setInsured_name((hsmp.get("r_insured_name") == null) ? "" : hsmp.get(
				"r_insured_name").trim());
		setDiv_option_desc2((hsmp.get("r_div_option_desc2") == null) ? ""
				: hsmp.get("r_div_option_desc2").trim());
		setCal_date((hsmp.get("r_cal_date") == null) ? "" : hsmp.get(
				"r_cal_date").trim());
		setAccumulated_div((hsmp.get("r_accumulated_div") == null) ? "" : hsmp
				.get("r_accumulated_div").trim());
		setDiv_int((hsmp.get("r_div_int") == null) ? "" : hsmp.get("r_div_int")
				.trim());
		setDiv_total((hsmp.get("r_div_total") == null) ? "" : hsmp.get(
				"r_div_total").trim());
		setLast_div_dur((hsmp.get("r_last_div_dur") == null) ? "" : hsmp.get(
				"r_last_div_dur").trim());
		setDiv_this_year((hsmp.get("r_div_this_year") == null) ? "" : hsmp.get(
				"r_div_this_year").trim());
	}

	// 右鍵-> source -> generate getter and setter
	public String getResult_ind() {
		return result_ind;
	}

	public void setResult_ind(String result_ind) {
		String oldValue = this.result_ind;
		this.result_ind = result_ind;
		firePropertyChange("result_ind", oldValue, this.result_ind);
	}

	public String getPolicy_no() {
		return policy_no;
	}

	public void setPolicy_no(String policy_no) {
		String oldValue = this.policy_no;
		this.policy_no = policy_no;
		firePropertyChange("policy_no", oldValue, this.policy_no);
	}

	public String getPo_sts_code() {
		return po_sts_code;
	}

	public void setPo_sts_code(String po_sts_code) {
		String oldValue = this.po_sts_code;
		this.po_sts_code = po_sts_code;
		firePropertyChange("po_sts_code", oldValue, this.po_sts_code);
	}

	public String getPo_sts_desc() {
		return po_sts_desc;
	}

	public void setPo_sts_desc(String po_sts_desc) {
		String oldValue = this.po_sts_desc;
		this.po_sts_desc = po_sts_desc;
		firePropertyChange("po_sts_desc", oldValue, this.po_sts_desc);
	}

	public String getDiv_option() {
		return div_option;
	}

	public void setDiv_option(String div_option) {
		String oldValue = this.div_option;
		this.div_option = div_option;
		firePropertyChange("div_option", oldValue, this.div_option);
	}

	public String getDiv_option_desc1() {
		return div_option_desc1;
	}

	public void setDiv_option_desc1(String div_option_desc1) {
		String oldValue = this.div_option_desc1;
		this.div_option_desc1 = div_option_desc1;
		firePropertyChange("div_option_desc1", oldValue, this.div_option_desc1);
	}

	public String getApplicant_name() {
		return applicant_name;
	}

	public void setApplicant_name(String applicant_name) {
		String oldValue = this.applicant_name;
		this.applicant_name = applicant_name;
		firePropertyChange("applicant_name", oldValue, this.applicant_name);
	}

	public String getInsured_name() {
		return insured_name;
	}

	public void setInsured_name(String insured_name) {
		String oldValue = this.insured_name;
		this.insured_name = insured_name;
		firePropertyChange("insured_name", oldValue, this.insured_name);
	}

	public String getDiv_option_desc2() {
		return div_option_desc2;
	}

	public void setDiv_option_desc2(String div_option_desc2) {
		String oldValue = this.div_option_desc2;
		this.div_option_desc2 = div_option_desc2;
		firePropertyChange("div_option_desc2", oldValue, this.div_option_desc2);
	}

	public String getCal_date() {
		return cal_date;
	}

	public void setCal_date(String cal_date) {
		String oldValue = this.cal_date;
		this.cal_date = cal_date;
		firePropertyChange("cal_date", oldValue, this.cal_date);
	}

	public String getAccumulated_div() {
		return accumulated_div;
	}

	public void setAccumulated_div(String accumulated_div) {
		String oldValue = this.accumulated_div;
		this.accumulated_div = accumulated_div;
		firePropertyChange("accumulated_div", oldValue, this.accumulated_div);
	}

	public String getDiv_int() {
		return div_int;
	}

	public void setDiv_int(String div_int) {
		String oldValue = this.div_int;
		this.div_int = div_int;
		firePropertyChange("div_int", oldValue, this.div_int);
	}

	public String getDiv_total() {
		return div_total;
	}

	public void setDiv_total(String div_total) {
		String oldValue = this.div_total;
		this.div_total = div_total;
		firePropertyChange("div_total", oldValue, this.div_total);
	}

	public String getLast_div_dur() {
		return last_div_dur;
	}

	public void setLast_div_dur(String last_div_dur) {
		String oldValue = this.last_div_dur;
		this.last_div_dur = last_div_dur;
		firePropertyChange("last_div_dur", oldValue, this.last_div_dur);
	}

	public String getDiv_this_year() {
		return div_this_year;
	}

	public void setDiv_this_year(String div_this_year) {
		String oldValue = this.div_this_year;
		this.div_this_year = div_this_year;
		firePropertyChange("div_this_year", oldValue, this.div_this_year);
	}

}
